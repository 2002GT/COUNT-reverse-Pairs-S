#include <iostream>
#include<vector>

using namespace std;

 int reverse_pair(vector<int>arr ){
     int pairs = 0;
     
     for(int i=0; i<n; i++){
         for(int j=i+1; j<n; j++){
             pairs++;
         }
         return pairs;
     }
 }

int main()
{
    vector<int>arr;
    arr ={1,3,2,3.1};
    cout<<reverse_pair(arr)

    return 0;
}
